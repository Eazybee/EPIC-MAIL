{
  "name": "server",
  "version": "1.0.0",
  "description": "Epic Mail API",
  "main": "src/index.js",
  "scripts": {
    "test": "mocha SERVER/v1/test --require @babel/polyfill --require @babel/register --recursive --exit",
    "build": "babel SERVER/v1/src --out-dir SERVER/v1/build/dist --exit",
    "dev-start": "babel-watch SERVER/v1/src/controller/route.js",
